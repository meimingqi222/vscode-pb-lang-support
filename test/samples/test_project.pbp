[Project]
Name=TestProject
Version=1.0.0
Author=TestAuthor

[Files]
Source1=Sources/main.pb
Source2=Includes/utils.pbi
Source3=Includes/constants.pbi

[Build]
Executable=TestProject.exe
Target=Windows
EnableDebugger=1
EnableUnicode=1
EnableThreads=1
EnableOnError=1
EnablePurifier=1
EnableConstantFolding=1
EnableInlineASM=1
EnableExplicit=1
EnableOptimizer=1

[Libraries]
Library1=user32.lib
Library2=kernel32.lib
Library3=gdi32.lib

[Compiler]
Subsystem=Windows
CommandLine=/QUIET /STANDBY

[Resources]
Icon=icon.ico
Manifest=manifest.xml