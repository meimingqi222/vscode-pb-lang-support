{
  "comments": {
    "lineComment": ";"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*(Procedure(?:C|DLL|CDLL)?|If|For|While|Repeat|Select|Structure|Interface|Enumeration|Module|DeclareModule)\\b",
      "end": "^\\s*(EndProcedure|EndIf|Next|Wend|Until|EndSelect|EndStructure|EndInterface|EndEnumeration|EndModule|EndDeclareModule)\\b"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(?i:(Procedure(?:C|DLL|CDLL)?\\b|Module\\b|DeclareModule\\b|Structure\\b|Interface\\b|Enumeration\\b|If\\b|For\\b|ForEach\\b|While\\b|Repeat\\b|Select\\b|With\\b|Macro\\b|DataSection\\b))",
    "decreaseIndentPattern": "^\\s*(?i:(EndProcedure\\b|EndModule\\b|EndDeclareModule\\b|EndStructure\\b|EndInterface\\b|EndEnumeration\\b|EndIf\\b|Next\\b|Wend\\b|Until\\b|ForEver\\b|EndSelect\\b|EndWith\\b|EndMacro\\b|EndDataSection\\b))"
  },
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
  "autocomplete": {
    "triggerCharacters": [".", "(", " "]
  }
}
