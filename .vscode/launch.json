{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension (F5)",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "preLaunchTask": "${defaultBuildTask}",
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}

